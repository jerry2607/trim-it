<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>logincheck</servlet-name>
        <servlet-class>logincheck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signupdata</servlet-name>
        <servlet-class>signupdata</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signupdatafinal</servlet-name>
        <servlet-class>signupdatafinal</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signafterimage</servlet-name>
        <servlet-class>signafterimage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addtokart</servlet-name>
        <servlet-class>addtocart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addtocart</servlet-name>
        <servlet-class>addtocart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>removefromcart</servlet-name>
        <servlet-class>removefromcart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>logout</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>logincheck</servlet-name>
        <url-pattern>/logincheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signupdata</servlet-name>
        <url-pattern>/signupdata</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signupdatafinal</servlet-name>
        <url-pattern>/signupdatafinal</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signafterimage</servlet-name>
        <url-pattern>/signafterimage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addtokart</servlet-name>
        <url-pattern>/addtokart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addtocart</servlet-name>
        <url-pattern>/addtocart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>removefromcart</servlet-name>
        <url-pattern>/removefromcart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
